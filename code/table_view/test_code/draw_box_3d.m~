function draw_box_3d(h)

[xmin ymin zmin xmax ymax zmax] = deal(h(1), h(2), h(3), h(4), h(5), h(6));
p1 = [xmin ymin zmin];
p2 = [xmax ymin zmin];
p3 = [xmax ymax zmin];
p4 = [xmin ymax zmin];
p5 = [xmin ymin zmax];
p6 = [xmax ymin zmax];
p7 = [xmax ymax zmax];
p8 = [xmin ymax zmax];

% Bottom squ
plot3([p1(1) p2(1)],[p1(2) p2(2)],[p1(3) p2(3)]); hold on;
plot3([p2(1) p3(1)],[p2(2) p3(2)],[p2(3) p3(3)]); hold on;
plot3([p3(1) p4(1)],[p3(2) p4(2)],[p3(3) p4(3)]); hold on;
plot3([p4(1) p1(1)],[p4(2) p1(2)],[p4(3) p1(3)]); hold on;
% keyboard;

end